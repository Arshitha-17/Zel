<div class="container mt-5 pt-4">
    <!-- Button trigger modal -->
<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal">
    Add Category
  </button>
  <% if (typeof message !== 'undefined') { %>
    <div class="alert alert-danger mt-3" role="alert">
      <%= message %>
    </div>
    <% } %>
    <!-- <a class="btn btn-secondary mb-3" data-bs-toggle="modal" data-bs-target="#exampleModalLong">Add Category</a> -->

    <table class="table table-striped" style="color:#000000;">
    <thead>
        <tr>
        <th scope="col"style="color:#f4eeee;">No</th>
        <th scope="col" style="color:#000000;" >Category Name</th>
        <th scope="col" style="color:#000000;" >Offer Price</th>
        <th scope="col" style="color:#000000;">Options</th>
        </tr>
    </thead>
    <% categories.forEach(function(category, index) { %>
        <tr>
        <th scope="row"><%= index + 1 %></th>
        <td style="color:#969292;" ><%= category.category %></td>
        <td style="color:#969292;" ><%= category.categoryOffer %></td>
        <td><a href="/admin/deteleCategory?id=<%= category._id %>" class="btn btn-danger" onclick="showtoast()">Delete</a></td>
        </tr>
        <% }); %>
    </table>
</div>



  
  <!-- Modal -->
  <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h1 class="modal-title fs-5" id="exampleModalLabel">Modal title</h1>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          
            <form action="/admin/addcategory" method="post">
                <div class="modal-body">
                        <div class="form-group">
                            <label for="categoryInput" class="my-2 " >Category:</label>
                            <input type="text" class="form-control form-control-sm my-2" id="categoryInput" name="category" placeholder="Enter Category">
                        </div>
                        <div class="form-group">
                            <label for="categoryOffer" class="my-2 " >Category Offer:</label>
                            <input type="number" class="form-control form-control-sm my-2" id="categoryOffer" name="offer" placeholder="Enter Category Offer">
                        </div>
                </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
          <button type="submit" class="btn btn-primary">Add</button>
        </div>
    </form>
      </div>
    </div>
